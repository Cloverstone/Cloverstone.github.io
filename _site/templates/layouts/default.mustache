			{{>menu}}
		</div>
	</nav>
	<section class="wrapper-sm">
		<div class="container">
			<div class="row">
				<div id="content">
					{{>content}}
				</div>
			</div>
		</div>
	</section>
	{{>footer}}
</body>
</html>
